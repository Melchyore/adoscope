    "@adonisjs/sink": "^1.0.17",
    "@adonisjs/websocket": "^1.0.11",
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "@types/bluebird": "^3.5.26",
    "@types/ioredis": "^4.0.10",
