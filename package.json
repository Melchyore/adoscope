{
  "name": "adoscopejs",
  "version": "0.4.0",
  "description": "Debugger for Adonisjs-based applications",
  "main": "index.js",
  "scripts": {
    "mix": "cross-env NODE_ENV=development webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "test": "node japaFile.js",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Melchyore/adoscopejs.git"
  },
  "keywords": [
    "adonis",
    "adonisjs",
    "adoscope",
    "adoscopejs",
    "debugger",
    "debug_tool",
    "test",
    "applications"
  ],
  "author": "Paradox",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Melchyore/adoscopejs/issues"
  },
  "homepage": "https://github.com/Melchyore/adoscopejs#readme",
  "dependencies": {
    "@types/lodash": "^4.14.123",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^11.13.7",
    "@types/node-schedule": "^1.2.3",
    "@types/on-finished": "^2.3.1",
    "@types/path-to-regexp": "^1.7.0",
    "@types/pluralize": "0.0.29",
    "@types/pretty-ms": "^4.0.0",
    "lodash": "^4.17.11",
